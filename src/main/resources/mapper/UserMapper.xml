<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.coderman.concurrent.mapper.UserMapper">
    <select id="selectAll" resultType="com.coderman.concurrent.model.User">
        select * from user
    </select>
    <select id="selectById" resultType="com.coderman.concurrent.model.User">
        select * from user where id=#{id}
    </select>
</mapper>
